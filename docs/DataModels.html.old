                                    <br />
                                    "tender":{
                                </p>
                                <p dir="ltr">
                                    "procurementMethodModalities": [
                                </p>
                                <p dir="ltr">
                                    "electronicAuction"
                                </p>
                                <p dir="ltr">
                                    ],
                                    <br />
                                    "electronicAuctions": {
                                    <br />
                                    "details": [
                                    <br />
